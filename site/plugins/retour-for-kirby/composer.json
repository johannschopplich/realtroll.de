{
    "name": "distantnative/retour-for-kirby",
    "description": "Redirects and error tracking for Kirby",
    "type": "kirby-plugin",
    "version": "3.0.0",
    "license": "MIT",
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "redirects",
        "errors",
        "routing"
    ],
    "authors": [
        {
            "name": "Nico Hoffmann",
            "email": "on-github@nhoffmann.com"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.2"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "distantnative\\Retour\\": "models/"
        }
    }
}
