{
  "require": {
    "php": ">=7.4.0",
    "getkirby/cms": "^3.0",
    "johannschopplich/kirby-extended": "^2.0",
    "johannschopplich/kirby-hashed-assets": "^2.0"
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S 127.0.0.1:8000 -t public/ server.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "kirby-cms-path": false
  }
}
